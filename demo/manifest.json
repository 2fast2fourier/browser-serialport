{
  "manifest_version": 2,

  "name": "Browser SerialPort Component Test",
  "description": "Robots in the browser. Just like node-serialport but for browser apps.",
  "version": "1.0",
  "app": {
    "background": {
      "scripts": ["extentionAssets/background.js"]
    }
  },
  "icons": {
    "16": "extentionAssets/icon-16x16.jpeg",
    "128": "extentionAssets/icon-128x128.jpeg"
  },
  
  "permissions": [
    "serial"
  ]
  
}
